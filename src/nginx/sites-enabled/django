upstream dynamic {
  server 127.0.0.1:8000       weight=5;
}

server {
  listen 80;
  server_name localhost;
  charset utf-8;

  # serve static fiels
  location /static {
      alias /code/static;
  }

  # serve media files
  location /media/ {
    alias /code/media/;
  }

  # Finally, send all non-media request to the Django Server
  location / {
    try_files $uri @wsgi;
  }

  location @wsgi {
    include uwsgi_params;
    uwsgi_pass 127.0.0.1:8000;
  }
}